{
    "name": "project/project",
    "description": "Description",
    "keywords": ["php", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Name",
            "email": "email@example.com"
        }
    ],
    "require": {
        "php": "^8.2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13.7",
        "pestphp/pest": "^2.28.1",
        "phpstan/phpstan": "^1.10.50",
        "symfony/var-dumper": "^6.4.0|^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Company\\Project\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
    }
}
